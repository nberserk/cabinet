{
  "manifest_version": 3,
  "name": "Tree Tabs",
  "version": "1.0",
  "description": "Hierarchical tab management",
  "permissions": ["tabs", "sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Tree Tabs"
  }
}
